<?xml version="1.0"?>
<odoo>
    <record id="card_view_form_list" model="ir.ui.view">
        <field name="name">Card Form</field>
        <field name="model">finance.card.view</field>
        <field name="arch" type="xml">
            <form string="会员信息">
                <sheet>
                    <group>
                        <field name="cardTitle"/>
                        <field name="cardType"/>
                        <field name="cardSummary"/>
                        <field name="cardUser"/>
                        <field name="cardTime"/>
                        <field name="cardWay"/>
                        <field name="cardEndTime"/>
                        <field name="cardIsTop"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="card_view_tree_list" model="ir.ui.view">
        <field name="name">Card List</field>
        <field name="model">finance.card.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="cardTitle"/>
                <field name="cardType"/>
                <field name="cardSummary"/>
                <field name="cardUser"/>
                <field name="cardTime"/>
                <field name="cardWay"/>
                <field name="cardEndTime"/>
                <field name="cardIsTop"/>
            </tree>
        </field>
    </record>
</odoo>