<?xml version="1.0"?>
<odoo>
    <record id="view_form_publ" model="ir.ui.view">
        <field name="name">Publ Form</field>
        <field name="model">surface.publ</field>
        <field name="arch" type="xml">
            <form string="Publ">
                <header>
                    <!-- 此处添加按钮 -->
<!--                    <button name="button_check_isbn" type="object" string="统计车位数量" />-->
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="numstart" />
                        <field name="range" />
                        <field name="chargemode" />
                        <field name="sharerange" />
                        <field name="sharemode" />
                        <field name="chargeformula" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_publ" model="ir.ui.view">
