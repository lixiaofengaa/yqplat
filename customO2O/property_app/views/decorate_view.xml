<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="view_form_property_decorate_register" model="ir.ui.view">
            <field name="name">Decorate Register Form</field>
            <field name="model">property.decorate.register</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="applicant"/>
                            <field name="phone"/>
                            <field name="description"/>
                            <field name="date_start"/>
                            <field name="date_end"/>
                            <field name="house"/>
                            <field name="state"/>
                        </group>
                    </sheet>
                    <chatter>
                        <div class="oe_chatter">
                            <field name="message_follower_ids" widget="mail_followers"/>
                            <field name="activity_ids" widget="mail_activity"/>
                            <field name="message_ids" widget="mail_thread"/>
                        </div>
                    </chatter>
                </form>
            </field>
        </record>
        <record id="view_tree_property_decorate_register" model="ir.ui.view">
            <field name="name">Decorate Register Tree</field>
            <field name="model">property.decorate.register</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="applicant"/>
                    <field name="phone"/>
                    <field name="description"/>
                    <field name="date_start"/>
                    <field name="date_end"/>
                    <field name="house"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
