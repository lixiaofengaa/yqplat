<?xml version="1.0"?>
<odoo>
    <record id="move_view_form_list" model="ir.ui.view">
        <field name="name">Move Form</field>
        <field name="model">business.move.view</field>
        <field name="arch" type="xml">
            <form string="入驻商信息">
                <sheet>
                    <group>
                        <field name="moveTitle"/>
                        <field name="moveType"/>
                        <field name="moveSummary"/>
                        <field name="moveUser"/>
                        <field name="moveTime"/>
                        <field name="moveWay"/>
                        <field name="moveEndTime"/>
                        <field name="moveIsTop"/>
                        <field name="moveAim"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="move_view_tree_list" model="ir.ui.view">
        <field name="name">Move List</field>
        <field name="model">business.move.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="moveTitle"/>
                <field name="moveType"/>
                <field name="moveSummary"/>
                <field name="moveUser"/>
                <field name="moveTime"/>
                <field name="moveWay"/>
                <field name="moveEndTime"/>
                <field name="moveIsTop"/>
                <field name="moveAim"/>
            </tree>
        </field>
    </record>
</odoo>