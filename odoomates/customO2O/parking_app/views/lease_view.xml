<?xml version="1.0"?>
<odoo>
    <record id="view_form_car" model="ir.ui.view">
        <field name="name">Car Form</field>
        <field name="model">parking.car</field>
        <field name="arch" type="xml">
            <form string="Car">
                <header>
                    <!-- 此处添加按钮 -->
<!--                    <button name="button_check_isbn" type="object" string="统计车位数量" />-->
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="nameId" />
                        <field name="carId" />
                        <field name="car" />
                        <field name="date_end" />
                        <field name="time" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_car" model="ir.ui.view">
        <field name="name">Car List</field>
        <field name="model">parking.car</field>
        <field name="arch" type="xml">
<!--            <button name="action_confirm_all_sale_ordesar" string="确认" type="action" />-->
            <tree>
                <field name="name" />
                <field name="nameId" />
                <field name="carId" />
                <field name="car" />
                <field name="date_end" />
                <field name="time" />
            </tree>
        </field>
    </record>




    <record id="view_form_handle" model="ir.ui.view">
        <field name="name">Handle Form</field>
        <field name="model">parking.handle</field>
        <field name="arch" type="xml">
            <form string="Handle">
                <header>
                    <!-- 此处添加按钮 -->
<!--                    <button name="button_check_isbn" type="object" string="统计车位数量" />-->
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="customer" />
                        <field name="space" />
                        <field name="date_start" />
                        <field name="date_end" />
                        <field name="month" />
                        <field name="unit" />
                        <field name="cost" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_handle" model="ir.ui.view">
        <field name="name">Handle List</field>
        <field name="model">parking.handle</field>
        <field name="arch" type="xml">
<!--            <button name="action_confirm_all_sale_ordesar" string="确认" type="action" />-->
            <tree>
                <field name="name" />
                <field name="customer" />
                <field name="space" />
                <field name="date_start" />
                <field name="date_end" />
                <field name="month" />
                <field name="unit" />
                <field name="cost" />
            </tree>
        </field>
    </record>

</odoo>