<?xml version="1.0"?>
<odoo>
    <record id="view_form_temporary" model="ir.ui.view">
        <field name="name">Temporary Form</field>
        <field name="model">parking.temporary</field>
        <field name="arch" type="xml">
            <form string="Temporary">
                <header>
                    <!-- 此处添加按钮 -->
<!--                    <button name="button_check_isbn" type="object" string="统计车位数量" />-->
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="customer" />
                        <field name="category" />
                        <field name="date_start" />
                        <field name="date_end" />
                        <field name="unit" />
                        <field name="hour" />
                        <field name="total" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_temporary" model="ir.ui.view">
        <field name="name">Temporary List</field>
        <field name="model">parking.temporary</field>
        <field name="arch" type="xml">
<!--            <button name="action_confirm_all_sale_ordesar" string="确认" type="action" />-->
            <tree>
                <field name="name" />
                <field name="customer" />
                <field name="category" />
                <field name="date_start" />
                <field name="date_end" />
                <field name="unit" />
                <field name="hour" />
                <field name="total" />
            </tree>
        </field>
    </record>

</odoo>