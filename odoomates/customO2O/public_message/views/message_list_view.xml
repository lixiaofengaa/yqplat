<?xml version="1.0"?>
<odoo>
    <record id="view_form_list" model="ir.ui.view">
        <field name="name">Message Form</field>
        <field name="model">message.list.view</field>
        <field name="arch" type="xml">
            <form string="信息">
                <sheet>
                    <group>
                        <field name="msgTitle"/>
                        <field name="msgType"/>
                        <field name="msgSummary"/>
                        <field name="msgUser"/>
                        <field name="msgTime"/>
                        <field name="msgWay"/>
                        <field name="msgEndTime"/>
                        <field name="msgIsTop"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_list" model="ir.ui.view">
        <field name="name">Message List</field>
        <field name="model">message.list.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="msgTitle"/>
                <field name="msgType"/>
                <field name="msgSummary"/>
                <field name="msgUser"/>
                <field name="msgTime"/>
                <field name="msgWay"/>
                <field name="msgEndTime"/>
                <field name="msgIsTop"/>
            </tree>
        </field>
    </record>
</odoo>