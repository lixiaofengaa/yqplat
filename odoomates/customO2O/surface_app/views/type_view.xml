<?xml version="1.0"?>
<odoo>
    <record id="view_form_type" model="ir.ui.view">
        <field name="name">Type Form</field>
        <field name="model">surface.type</field>
        <field name="arch" type="xml">
            <form string="Type">
                <header>
                    <!-- 此处添加按钮 -->
<!--                    <button name="button_check_isbn" type="object" string="统计车位数量" />-->
                </header>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="unit" />
                        <field name="remarks" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_type" model="ir.ui.view">
        <field name="name">Type List</field>
        <field name="model">surface.type</field>
        <field name="arch" type="xml">
<!--            <button name="action_confirm_all_sale_ordesar" string="确认" type="action" />-->
            <tree>
                <field name="name" />
                <field name="unit" />
                <field name="remarks" />
            </tree>
        </field>
    </record>

</odoo>