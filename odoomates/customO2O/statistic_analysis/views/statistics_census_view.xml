<?xml version="1.0"?>
<odoo>
    <record id="census_view_form_list" model="ir.ui.view">
        <field name="name">Census Form</field>
        <field name="model">statistics.census.view</field>
        <field name="arch" type="xml">
            <form string="会员信息">
                <sheet>
                    <group>
                        <field name="censusTitle"/>
                        <field name="censusType"/>
                        <field name="censusSummary"/>
                        <field name="censusUser"/>
                        <field name="censusTime"/>
                        <field name="censusWay"/>
                        <field name="censusEndTime"/>
                        <field name="censusIsTop"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="census_view_tree_list" model="ir.ui.view">
        <field name="name">Census List</field>
        <field name="model">statistics.census.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="censusTitle"/>
                <field name="censusType"/>
                <field name="censusSummary"/>
                <field name="censusUser"/>
                <field name="censusTime"/>
                <field name="censusWay"/>
                <field name="censusEndTime"/>
                <field name="censusIsTop"/>
            </tree>
        </field>
    </record>
</odoo>