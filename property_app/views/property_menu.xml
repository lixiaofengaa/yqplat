        <act_window id="action_property_maintain" name="维修申请列表" res_model="property.maintain" view_mode="tree,form"/>
        <menuitem id="menu_property_maintain" name="维修申请" parent="menu_property_maintain_manager" sequence="1" action="action_property_maintain" />
        <!--维修受理-->
        <act_window id="action_property_maintain_handle" name="申请受理记录" res_model="property.maintain.handle" view_mode="tree,form"/>
