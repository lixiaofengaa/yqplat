        <!--登记受理-->
        <act_window id="action_property_decorate_handle" name="受理记录" res_model="property.decorate.handle" view_mode="tree,form"/>
        <menuitem id="menu_property_decorate_handle" name="受理记录" parent="menu_property_decorate_manager" sequence="2" action="action_property_decorate_handle" />
        <!--装修验收-->
        <act_window id="action_property_decorate_check" name="验收明细" res_model="property.decorate.check" view_mode="tree,form"/>
        <menuitem id="menu_property_decorate_check" name="装修验收" parent="menu_property_decorate_manager" sequence="3" action="action_property_decorate_check" />
        <!--装修收费-->
        <act_window id="action_property_decorate_pay" name="收费账单" res_model="property.decorate.pay" view_mode="tree,form"/>
        <menuitem id="menu_property_decorate_pay" name="装修收费" parent="menu_property_decorate_manager" sequence="4" action="action_property_decorate_pay" />
    <!--投诉建议-->
    <menuitem id="menu_property_suggest_manager" name="投诉建议" sequence="7" parent="menu_property" groups="property_group_user"/>
        <!--投诉记录-->
        <act_window id="action_property_suggest_record" name="投诉列表" res_model="property.charge.debt" view_mode="tree,form"/>
        <menuitem id="menu_property_suggest_record" name="投诉记录" parent="menu_property_suggest_manager" sequence="1" action="action_property_suggest_record" />
        <!--受理记录-->
        <act_window id="action_property_suggest_handle" name="受理列表" res_model="property.charge.debt" view_mode="tree,form"/>
        <menuitem id="menu_property_suggest_handle" name="投诉处理" parent="menu_property_suggest_manager" sequence="2" action="action_property_suggest_handle" />
        <!--回访记录-->
        <act_window id="action_property_suggest_visit" name="记录列表" res_model="property.charge.debt" view_mode="tree,form"/>
        <menuitem id="menu_property_suggest_visit" name="回访记录" parent="menu_property_suggest_manager" sequence="3" action="action_property_suggest_visit" />
</odoo>