<?xml version="1.0"?>
<odoo>
    <record id="consumer_view_form_list" model="ir.ui.view">
        <field name="name">Consumer Form</field>
        <field name="model">user.consumer.view</field>
        <field name="arch" type="xml">
            <form string="用户信息">
                <sheet>
                    <group>
                        <field name="consumerTitle"/>
                        <field name="consumerType"/>
                        <field name="consumerAge"/>
                        <field name="consumerSummary"/>
                        <field name="consumerUser"/>
                        <field name="consumerTime"/>
                        <field name="consumerIsTop"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="consumer_view_tree_list" model="ir.ui.view">
        <field name="name">Consumer List</field>
        <field name="model">user.consumer.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="consumerTitle"/>
                <field name="consumerType"/>
                <field name="consumerAge"/>
                <field name="consumerSummary"/>
                <field name="consumerUser"/>
                <field name="consumerTime"/>
                <field name="consumerIsTop"/>
            </tree>
        </field>
    </record>
</odoo>