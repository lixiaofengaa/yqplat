<?xml version="1.0"?>
<odoo>
    <record id="member_view_form_list" model="ir.ui.view">
        <field name="name">Member Form</field>
        <field name="model">user.member.view</field>
        <field name="arch" type="xml">
            <form string="会员信息">
                <sheet>
                    <group>
                        <field name="memberTitle"/>
                        <field name="memberType"/>
                        <field name="memberSummary"/>
                        <field name="memberUser"/>
                        <field name="memberTime"/>
                        <field name="memberWay"/>
                        <field name="memberEndTime"/>
                        <field name="memberIsTop"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="member_view_tree_list" model="ir.ui.view">
        <field name="name">Member List</field>
        <field name="model">user.member.view</field>
        <field name="arch" type="xml">
            <tree>
                <field name="memberTitle"/>
                <field name="memberType"/>
                <field name="memberSummary"/>
                <field name="memberUser"/>
                <field name="memberTime"/>
                <field name="memberWay"/>
                <field name="memberEndTime"/>
                <field name="memberIsTop"/>
            </tree>
        </field>
    </record>
</odoo>